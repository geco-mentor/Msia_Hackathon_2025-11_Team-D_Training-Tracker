{
    "name": "ai-training-tracker",
    "version": "1.0.0",
    "type": "module",
    "description": "AI Training Effectiveness Tracker - Full Stack Application",
    "scripts": {
        "dev": "node dev-start.js",
        "dev:backend": "cd backend && npm run dev",
